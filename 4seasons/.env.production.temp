# Temporary production environment for v0 deployment (no database)
NODE_ENV=production
JWT_SECRET=localonly-jwt-key-abcdefghijklmnopqrstuvwxyz1234567890abcdef
SESSION_SECRET=localonly-session-key-abcdefghijklmnopqrstuvwxyz1234567890
PORT=3000

# Disable database-dependent features
DISABLE_DATABASE=true

# MLS settings
MLS_PROXY_ENABLED=true

# Email settings (optional - will use fallback)
SMTP_HOST=
SMTP_USER=
SMTP_PASS=

# Security settings
ALLOWED_ORIGINS=*
CORS_ENABLED=true

# Performance settings
HOSTING_PROVIDER=vercel